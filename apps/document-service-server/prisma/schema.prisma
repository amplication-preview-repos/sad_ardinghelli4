datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Document {
  createdAt  DateTime @default(now())
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  identifier String?
  image      Json?
  pointer    String?
  step       String?
  updatedAt  DateTime @updatedAt
}
